package com.javaquestion;

public class NumWithooutLoop {
    public static void main(String[] args) {
        printNum(1);
    }

    public static void printNum(int num) {
        if (num <= 100) {
            System.out.println(num);
            num++;
            printNum(num);
        }
    }

}

/*
Print number from 1 to 100 without loop
*/
